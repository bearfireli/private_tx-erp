<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hntxrj.txerp.mapper.SystemSqlMapper">
    <select id="getSqlByName" resultType="java.lang.String">
        select sql_content
        from system_sql
        where sql_name = #{sqlName}
    </select>
</mapper>