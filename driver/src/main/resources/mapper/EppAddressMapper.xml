<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hntxrj.driver.mapper.EppAddressMapper">
    <select id="getEppAddress" resultType="com.hntxrj.driver.entity.EppAddress">
        select *
        from epp_address
        where compid = #{compid}
          and epp_code = #{eppCode}
    </select>
    <select id="getCountByEppCode" resultType="java.lang.Integer">
        select count(epp_code)
        from epp_address
        where epp_code = #{eppCode}
          and compid = #{compid}
    </select>
</mapper>