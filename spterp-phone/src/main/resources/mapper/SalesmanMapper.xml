<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hntxrj.mapper.SalesmanMapper">

    <select id="getSalesmanDropDown" resultType="com.hntxrj.vo.SalesmanDropDownVO">
        select SalesmanCode salesCode, ScaleName salesManName
        from SM_BusinessGroup
        where compid = #{compid} and RecStatus = 1
    </select>
</mapper>